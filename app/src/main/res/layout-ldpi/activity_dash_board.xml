<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/darker_gray"
    tools:context=".activity.DashBoardActivity">

    <LinearLayout
        android:id="@+id/layout_add_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0.5dp"
        android:paddingEnd="0dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_weight_DashBoardActivity"
        app:layout_constraintEnd_toStartOf="@+id/layout_del_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appbar_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/add"
            android:src="@drawable/logo_add" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/add"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_del_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/layout_food_DashBoardActivity"
        app:layout_constraintEnd_toStartOf="@+id/layout_healthcare_DashBoardActivity"
        app:layout_constraintStart_toEndOf="@+id/layout_add_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/appbar_DashBoardActivity">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/delete"
            android:src="@drawable/logo_delete" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/delete"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_healthcare_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0.5dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_breeding_DashBoardActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@+id/layout_del_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/appbar_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/healthcare"
            android:src="@drawable/logo_healthcare" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/healthcare"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_weight_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0.5dp"
        android:paddingEnd="0dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_goat_milk_DashBoardActivity"
        app:layout_constraintEnd_toStartOf="@+id/layout_food_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_add_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/weight"
            android:src="@drawable/logo_weight_main" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/weight"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_food_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/layout_expense_DashBoardActivity"
        app:layout_constraintEnd_toStartOf="@+id/layout_breeding_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@+id/layout_weight_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/layout_del_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/food_plan"
            android:src="@drawable/logo_food_plan" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/food_plan"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_breeding_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0.5dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_income_DashBoardActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@+id/layout_food_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/layout_healthcare_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/breeding"
            android:src="@drawable/logo_breeding" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/breeding"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <!--original expense-->
    <!--now goat milk-->
    <LinearLayout
        android:id="@+id/layout_goat_milk_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0.5dp"
        android:paddingEnd="0dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_reports_DashBoardActivity"
        app:layout_constraintEnd_toStartOf="@+id/layout_expense_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_weight_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/goat_milk"
            android:src="@drawable/logo_goat_milk" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/goat_milk"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <!--original income-->
    <!--now expense-->
    <LinearLayout
        android:id="@+id/layout_expense_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/layout_blank_DashBoardActivity"
        app:layout_constraintEnd_toStartOf="@+id/layout_income_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@+id/layout_goat_milk_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/layout_food_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/expense"
            android:src="@drawable/logo_expense" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/expense"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <!--original reports-->
    <!--now income-->
    <LinearLayout
        android:id="@+id/layout_income_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginBottom="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0.5dp"
        app:layout_constraintBottom_toTopOf="@+id/layout_setting_DashBoardActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@+id/layout_expense_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/layout_breeding_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/income"
            android:src="@drawable/logo_income" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/income"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <!--original goat milk-->
    <!--now reports-->
    <LinearLayout
        android:id="@+id/layout_reports_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0.5dp"
        android:paddingEnd="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/layout_blank_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_goat_milk_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/reports"
            android:scaleType="fitCenter"
            android:src="@drawable/logo_reports" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/reports"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_blank_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginTop="0.5dp"
        android:layout_marginEnd="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/layout_setting_DashBoardActivity"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@id/layout_reports_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/layout_expense_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_setting_DashBoardActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0.5dp"
        android:layout_marginTop="0.5dp"
        android:background="#FEFEFE"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingStart="0dp"
        android:paddingEnd="0.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_min="45dp"
        app:layout_constraintStart_toEndOf="@+id/layout_blank_DashBoardActivity"
        app:layout_constraintTop_toBottomOf="@+id/layout_income_DashBoardActivity"
        app:layout_constraintWidth_min="45dp">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:contentDescription="@string/master_setting"
            android:src="@drawable/logo_master_setting" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/master_setting"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/black" />
    </LinearLayout>

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar_DashBoardActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_DashBoardActivity"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </com.google.android.material.appbar.AppBarLayout>

</androidx.constraintlayout.widget.ConstraintLayout>